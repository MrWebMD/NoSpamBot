{
  "source": {
    "include": ["detection-modules", "embeds", "helpers", "mitigations", "reporting", "store"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "doc-template"
  }
}